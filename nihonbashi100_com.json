    "attr": "href",
    "regex": null,
    "ng_words": null
  },
  "detail": {
    "xpath": "//section[@id='main']//ul[contains(@class, 'contentHentryList')]/li/a",
    "attr": "href",
    "regex": null,
    "ng_words": null
  },
  "therapist": {
    "name": {
      "xpath": "//h1[contains(@class, 'entry-title-h')]/span[contains(@class, 'entry-title-span')]",
      "regex": null,
      "ng_words": null
    },
    "age": {
      "xpath": "//h1[contains(@class, 'entry-title-h')]/span[contains(@class, 'entry-title-span')]/span",
      "regex": "\\((\\d+)\\)",
      "ng_words": null
    },
    "height": {
      "xpath": null,
      "regex": null,
      "ng_words": null
    },
    "comment": {
      "xpath": "//div[contains(@class, 'entry-content')]/div[contains(@class, 'castContent')]",
      "regex": null,
      "ng_words": null
    },
    "x_account": {
      "xpath": null,
      "attr": null,
      "regex": null,
      "ng_words": null
    },
    "image": {
      "xpath": "//ul[contains(@class, 'cast-thumbnail')]/li[contains(@class, 'thumbnaillist-item')]/img",
      "attr": "src",
      "regex": null,
      "ng_words": null
    },
    "schedule": {
      "date": {
        "month": {
          "xpath": "//div[contains(@class, 'castSchedule')]/dl/dt",
          "regex": "(\\d{1,2})月"
        },
        "day": {
          "xpath": "//div[contains(@class, 'castSchedule')]/dl/dt",
          "regex": "(\\d{1,2})日"
        }
      },
      "time": {
        "start": {
          "xpath": "//div[contains(@class, 'castSchedule')]/dl/dd/div[contains(@class, 'scheduleTime')]",
          "regex": "^(\\d{1,2}):(\\d{1,2})"
        },
        "end": {
          "xpath": "//div[contains(@class, 'castSchedule')]/dl/dd/div[contains(@class, 'scheduleTime')]",
          "regex": "(\\d{1,2}):(\\d{1,2})$"
        }
      }
    }
  }
}

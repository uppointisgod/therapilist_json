{
  "url": "https://sr-himawari.com/",
  "refnavi_url": "https://osaka.refle.info/shop/secret-room/",
  "list_url": "https://sr-himawari.com/cast",
  "detail": {
    "xpath": "//ul[contains(@class, 'gallist')]/li/a",
    "attr": "href",
    "regex": null,
    "ng_words": null
  },
  "therapist": {
    "name": {
      "xpath": "//section[@id='profdata']/h3",
      "regex": "(.*)\\(",
      "ng_words": null
    },
    "age": {
      "xpath": "//section[@id='profdata']/h3",
      "regex": "\\((\\d{1,2})\\)",
      "ng_words": null
    },
    "height": {
      "xpath": null,
      "regex": null,
      "ng_words": null
    },
    "comment": {
      "xpath": "//section[@id='profdata']//dt[contains(text(), 'コメント')]/following-sibling::dd",
      "regex": null,
      "ng_words": null
    },
    "x_account": {
      "xpath": null,
      "attr": null,
      "regex": null,
      "ng_words": null
    },
    "image": {
      "xpath": "//div[@id='imageList']//img",
      "attr": "src",
      "regex": null,
      "ng_words": null
    },
    "schedule": {
      "date": {
        "month": {
          "xpath": "//div[@id='schebox']//ul[@class='pc']/li/p[@class='time']/preceding-sibling::p[@class='days']",
          "regex": "(\\d{1,2})/"
        },
        "day": {
          "xpath": "//div[@id='schebox']//ul[@class='pc']/li/p[@class='time']/preceding-sibling::p[@class='days']",
          "regex": "/(\\d{1,2})"
        }
      },
      "time": {
        "start": {
          "xpath": "//div[@id='schebox']//ul[@class='pc']/li/p[@class='time']",
          "regex": "^(\\d{1,2}):(\\d{1,2})"
        },
        "end": {
          "xpath": "//div[@id='schebox']//ul[@class='pc']/li/p[@class='time']",
          "regex": "(\\d{1,2}):(\\d{1,2})$"
        }
      }
    }
  }
}

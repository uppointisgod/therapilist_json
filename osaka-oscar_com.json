      "time": {
        "start": {
          "xpath": "//ul[contains(@class, 'weeklySchedule')]/li/p",
          "regex": "^(\\d{1,2}):(\\d{1,2})"
        },
        "end": {
          "xpath": "//ul[contains(@class, 'weeklySchedule')]/li/p",

      "ng_words": null
    },
    "age": {
      "xpath": "//div[@class='prof_right']/dl[@class='txt']//dt[contains(text(), '年齢')]/following-sibling::dd[1]",
      "regex": "(\\d{1,2})歳",
      "ng_words": null

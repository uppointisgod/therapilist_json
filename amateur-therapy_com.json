  "next_page_url": {
    "xpath": null,
    "attr": null,
    "regex": null,
    "ng_words": null
  },
  "detail": {
    "xpath": "//ul[contains(@class, 'ladyList')]/li/a",
    "attr": "href",
    "regex": null,
    "ng_words": null
  },
  "therapist": {
    "name": {
      "xpath": "//h2[@class='ladyName']/em",
      "regex": "(.*)（",
      "ng_words": null
    },
    "age": {
      "xpath": "//h2[@class='ladyName']/em",
      "regex": "（(\\d{1,2})歳）",
      "ng_words": null
    },
    "height": {
      "xpath": "//table[@id='profile']//tr/th[contains(text(), '身長')]/following-sibling::td",
      "regex": "(\\d{1,3})cm",
      "ng_words": null
    },
    "comment": {
      "xpath": "//table[@id='profile']//tr/th[contains(text(), 'お店から')]/following-sibling::td",
      "regex": null,
      "ng_words": null
    },
    "x_account": {
      "xpath": null,
      "attr": null,
      "regex": null,
      "ng_words": null
    },
    "image": {
      "xpath": "//div[@id='gallary']//ul[contains(@class, 'slides_container')]/slides_control//img",
      "attr": "src",
      "regex": null,
      "ng_words": null
    },
    "schedule": {
      "date": {
        "month": {
          "xpath": "//table[@id='schedule']//tr[contains(@class, 'subTtl')]/th",
          "regex": "(\\d{1,2})/"
        },
        "day": {
          "xpath": "//table[@id='schedule']//tr[contains(@class, 'subTtl')]/th",
          "regex": "/(\\d{1,2})"
        }
      },
      "time": {
        "start": {
          "xpath": "//table[@id='schedule']//tr/td[{index}]/em[contains(@class, 'time')]",
          "regex": "^(\\d{1,2}):(\\d{1,2})"
        },
        "end": {
          "xpath": "//table[@id='schedule']//tr/td[{index}]/em[contains(@class, 'time')]",
          "regex": "(\\d{1,2}):(\\d{1,2})$"
        }
      }
    }
  }
}
